        <Button
            ohos:height="24vp"
            ohos:width="24vp"
            ohos:align_parent_bottom="$id:layout1"
            ohos:align_parent_right="$id:layout1"
            ohos:background_element="$media:ic_arrow_right"/>
    </DependentLayout>

