<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:id="$+id:shoping_cart_order"
    ohos:width="match_parent"
    ohos:height="match_parent"
    ohos:orientation="vertical">
    <Text
        ohos:height="80px"
        ohos:width="300px"
        ohos:text="$string:submit_order_label"
        ohos:text_alignment="left"
        ohos:left_margin="20px"
        ohos:text_size="20vp"
        ohos:weight="6vp"
        />
    <Component
        ohos:width="match_parent"
        ohos:height="0.5vp"
        ohos:background_element="#FFE2E2EE"
        />
    <DependentLayout
        ohos:height="120px"
        ohos:width="match_parent"
        ohos:margin="10px">
        <Text
            ohos:height="60px"
            ohos:width="100px"
            ohos:id="$+id:default_addr"
            ohos:text="$string:default_label"
            ohos:background_element="#F93737"
            ohos:left_margin="20px"
            ohos:text_size="14vp"
            ohos:text_color="$color:white"
            ohos:weight="5vp"
            />
        <Text
            ohos:id="$+id:addr"
            ohos:height="60px"
            ohos:width="match_parent"
            ohos:left_margin="20px"
            ohos:text="$string:address_info"
            ohos:multiple_lines="true"
            ohos:right_of="$id:default_addr"
            ohos:text_size="14vp"
            ohos:weight="4vp"/>
        <Text
            ohos:id="$+id:name_phone"
            ohos:height="60px"
            ohos:width="match_parent"
            ohos:text="$string:phone_info"
            ohos:left_margin="20px"
            ohos:below="$id:addr"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            ohos:right_of="$id:default_addr"/>
    </DependentLayout>
    <DependentLayout
        ohos:height="80px"
        ohos:width="match_parent"
        ohos:alignment="left"
        ohos:margin="10px">
        <Text
            ohos:height="match_parent"
            ohos:width="300px"
            ohos:id="$+id:head_pro_money"
            ohos:text="$string:commodity_amount_label"
            ohos:left_margin="20px"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            />
        <Text
            ohos:id="$+id:pro_money"
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:right_of="$id:head_pro_money"
            ohos:text=" ￥2756.00"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            ohos:left_margin="200px"/>
    </DependentLayout>
    <Component
        ohos:width="match_parent"
        ohos:height="0.5vp"
        ohos:background_element="#FFFFFF"
        />
    <DependentLayout
        ohos:height="80px"
        ohos:width="match_parent"
        ohos:alignment="left"
        ohos:margin="10px">
        <Text
            ohos:height="match_parent"
            ohos:width="300px"
            ohos:id="$+id:head_pro_tansmoney"
            ohos:text="$string:freight_label"
            ohos:left_margin="20px"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            />
        <Text
            ohos:id="$+id:pro_tansmoney"
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:right_of="$id:head_pro_tansmoney"
            ohos:text=" ￥0.00"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            ohos:left_margin="200px"/>
    </DependentLayout>
    <Component
        ohos:width="match_parent"
        ohos:height="0.5vp"
        ohos:background_element="#FFFFFF"
        />
    <DependentLayout
        ohos:height="80px"
        ohos:width="match_parent"
        ohos:alignment="left"
        ohos:margin="10px">
        <Text
            ohos:height="match_parent"
            ohos:width="300px"
            ohos:id="$+id:head_pro_discountmoney"
            ohos:text="$string:subtraction_label"
            ohos:left_margin="20px"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            />
        <Text
            ohos:id="$+id:pro_discountmoney"
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:right_of="$id:head_pro_discountmoney"
            ohos:text="-￥317.00"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            ohos:left_margin="200px"/>
    </DependentLayout>
    <Component
        ohos:width="match_parent"
        ohos:height="0.5vp"
        ohos:background_element="#FFFFFF"
        />
    <DependentLayout
        ohos:height="80px"
        ohos:width="match_parent"
        ohos:alignment="left"
        ohos:margin="10px">
        <Text
            ohos:height="match_parent"
            ohos:width="300px"
            ohos:id="$+id:head_coupons"
            ohos:text="$string:coupon_label"
            ohos:left_margin="20px"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            />
        <Text
            ohos:id="$+id:coupons"
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:right_of="$id:head_coupons"
            ohos:text="$string:no_available_info"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            ohos:left_margin="200px"/>
    </DependentLayout>
    <Component
        ohos:width="match_parent"
        ohos:height="0.5vp"
        ohos:background_element="#FFFFFF"
        />
    <Component
        ohos:width="match_parent"
        ohos:height="0.5vp"
        ohos:background_element="#FFE2E2EE"
        />
    <DependentLayout
        ohos:weight="1"
        ohos:height="80px"
        ohos:width="match_parent"
        ohos:margin="10px">
        <Text
            ohos:height="80px"
            ohos:width="300px"
            ohos:id="$+id:myshopping_cart"
            ohos:text="$string:total_label"
            ohos:left_margin="30px"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            />
        <Text
            ohos:id="$+id:share_shoping_cart"
            ohos:height="80px"
            ohos:width="match_parent"
            ohos:right_of="$id:myshopping_cart"
            ohos:text=" :￥2439.00"
            ohos:text_size="14vp"
            ohos:weight="4vp"
            ohos:left_margin="200px"/>
    </DependentLayout>
    <DirectionalLayout
        ohos:height="200px"
        ohos:width="match_parent"
        ohos:left_padding="20px"
        ohos:right_padding="20px"
        ohos:background_element="#FAFAFA"
        ohos:alignment="center"
        ohos:orientation="horizontal"
        ohos:align_parent_bottom="true">
        <Text
            ohos:id="$+id:order_money"
            ohos:width="360px"
            ohos:height="160px"
            ohos:text_alignment="center"
            ohos:left_margin="80px"
            ohos:text_size="16vp"
            ohos:text="￥2439.00"
            ohos:text_color="#F93939"
            ohos:weight="6"/>
        <Text
            ohos:id="$+id:commit_order"
            ohos:width="360px"
            ohos:height="160px"
            ohos:background_element="$graphic:corner_bg_comment"
            ohos:text_alignment="center"
            ohos:left_margin="100px"
            ohos:right_of="$id:total_money"
            ohos:text_color="white"
            ohos:text_size="20vp"
            ohos:text="$string:submit_order_label"
            ohos:weight="6"/>
    </DirectionalLayout>
</DirectionalLayout>