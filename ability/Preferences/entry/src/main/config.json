{
  "app": {
    "bundleName": "ohos.samples.preferences",
    "version": {
      "code": 2000000,
      "name": "2.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Release"
    },
    "type": "normal"
  },
  "deviceConfig": {},
  "module": {
    "package": "ohos.samples.preferences",
    "name": ".Preferences",
    "reqCapabilities": [
      "video_support"
    ],
    "deviceType": [
      "default"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "visible": true,
        "formsEnabled": false,
        "name": "ohos.samples.preferences.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/splash/splash",
          "pages/login/login",
          "pages/main/main"
        ],
        "name": "default",
        "window": {
          "designWidth": 750,
          "autoDesignWidth": false
        }
      }
    ]
  }
}